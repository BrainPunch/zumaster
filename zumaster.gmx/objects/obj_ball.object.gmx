          <argument>
            <kind>1</kind>
            <string>/// Combine together

if (!instance_exists(id) || !instance_exists(other)) exit;

var doConnect = !ds_map_exists(adjacent, other.id);
if (!inSameClump(id, other.id)) {
    var mySize = countClumpSize(id);
    var otherSize = countClumpSize(other.id);
    
    var minSize = min(mySize, otherSize);
    var maxSize = max(mySize, otherSize);
    var ratioSize = minSize / maxSize;
    
    if (minSize &gt; 12) {
        //Large clumps don't connect
        doConnect = false;
    } else if (ratioSize &gt; 0.3 &amp;&amp; minSize &gt; 6) {
        //Similarly sized clumps don't connect
        doConnect = false;
    }
}

if (doConnect) {
    var joint = physics_joint_distance_create(id, other,
        phy_position_x, phy_position_y,
        other.phy_position_x, other.phy_position_y,
        false);
    ds_map_add(adjacent, other.id, joint);
    ds_map_add(other.adjacent, id, joint);
    
    checkForCombo(id);
}
</string>
